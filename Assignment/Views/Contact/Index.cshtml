                                    <fieldset class="form-group col-md-12">
                                        <input type="text" placeholder="Họ và tên" class="form-control form-control-lg">
                                    </fieldset>
                                    <fieldset class="form-group col-md-12">
                                        <input type="email" placeholder="Email" class="form-control form-control-lg">
                                    </fieldset>
                                    <fieldset class="form-group col-md-12">
                                        <textarea placeholder="Nội dung" class="form-control form-control-lg" rows="5"></textarea>
                                    </fieldset>
                                    <div class="col-md-12">
                                        <button type="submit" class="btn btn-primary btn-comment">Gửi liên hệ</button>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
